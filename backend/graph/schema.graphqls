type Query {
  getData(name: String!): DataSection
}

#var DataSections []DataSection - defined & initialized in schema.resolvers.go
type DataSection {
  name: String!
  items: [Item!]!
}

type Item {
  keywords: [String]
  content: Content
}

type Content {
  #profesional-exp
  name: String
  role: String
  start: String
  end: String
  DateDifference: String
  tasks: [String]
  #education
  title: String
  url: String
}